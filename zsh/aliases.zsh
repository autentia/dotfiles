# GENERAL
alias ..='cd ../'
alias ...='cd ../../'
alias ....='cd ../../../'

alias l='ls -laGFhS'
alias ls='ls -aGFhS'
alias lg='l | grep --color=auto'

alias tailf='tail -f'

alias cls='clear'

alias ff='findfile'

alias fd='finddir'

alias s='grep_filtered'

# UPDATE PROFILE
alias zp='. ~/.zshrc'

# Go to Dotfiles
alias df="cd ~/.dotfiles; vim ./"
